{
	"name": "Behance",
	"url": "https://www.behance.net/v2/oauth",
	"oauth2": {
		"authorize": "/authenticate",
		"access_token": {
			"url": "/token",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string",
			"scope": "string"
		}
	}
}