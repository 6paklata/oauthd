{
	"name": "Runkeeper",
	"url": "https://runkeeper.com/apps",
	"oauth2": {
		"authorize": "/authorize",
		"access_token": {
			"url": "/token",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string"
		}
	},
	"href": {
		"provider": "http://runkeeper.com/",
		"docs": "http://developer.runkeeper.com/healthgraph/overview",
		"keys": "http://runkeeper.com/partner/applications"
	}
}