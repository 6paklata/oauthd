{
	"name": "Disqus",
	"url": "https://disqus.com/api/oauth/2.0",
	"oauth2": {
		"authorize": "/authorize/",
		"access_token": {
			"url": "/access_token/",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string",
			"scope": "string"
		}
	}
}