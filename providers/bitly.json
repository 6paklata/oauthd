{
	"name": "Bitly",
	"url": "https:/",
	"oauth2": {
		"authorize": "/bitly.com/oauth/authorize",
		"access_token": {
			"url": "/api-ssl.bitly.com/oauth/access_token",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string"
		}
	}
}