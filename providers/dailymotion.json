{
	"name": "Dailymotion",
	"url": "https://api.dailymotion.com/oauth",
	"oauth2": {
		"authorize": {
			"url": "/authorize",
			"query": {
				"response_type": "code",
				"client_id": "{client_id}",
				"redirect_uri": "{{callback}}"
			}
		},
		"access_token": {
			"url": "/token",
			"method": "post"
		},
		"request": {
			"query": {
				"access_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string"
		}
	}
}