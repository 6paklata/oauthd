{
	"name": "Salesforce",
	"url": "https://login.salesforce.com/services/oauth2",
	"oauth2": {
		"authorize": "/authorize",
		"access_token": {
			"url": "/token",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string",
			"scope": "string"
		}
	}
}