{
	"name": "Flickr",
	"url": "http://www.flickr.com/services/oauth",
	"oauth1": {
		"request_token": "/request_token",
		"authorize": {
			"url": "/authorize",
			"query": {
				"perms": "{permissions}"
			}
		},
		"access_token": "/access_token",
		"request": {
			"put": {
				"api_key": "{client_id}"
			},
			"post": {
				"api_key": "{client_id}"
			},
			"delete": {
				"api_key": "{client_id}"
			},
			"query": {
				"api_key": "{client_id}",
				"format": "json",
				"nojsoncallback": "1"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string",
			"permissions": {
				"type": "string",
				"cardinality": "1",
				"values": {
					"read": "Read access to the api",
					"write": "Read and write access to the api",
					"delete": "Read, write and deletion access to the api"
				}
			}
		}
	}
}