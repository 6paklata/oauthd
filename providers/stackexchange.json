{
	"name": "Stackexchange",
	"api_url": "https://api.stackexchange.com/2.1/",
	"url": "https://stackexchange.com",
	"oauth2": {
		"authorize": "/oauth",
		"access_token": {
			"url": "/oauth/access_token",
			"method": "post"
		},
		"request": {
			"query": {
				"access_token": "{{token}}",
				"key": "{key}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string",
			"scope": {
				"type": "string",
				"separator": " ",
				"cardinality": "*",
				"values": {
					"read_inbox": "access a user's global inbox",
					"no_expiry": "access_token's with this scope do not expire",
					"write_access": "perform write operations as a user",
					"private_info": "access full history of a user's private actions on the site."
				}
			}
		}
	},
	"parameters": {
		"key": "string"
	}
}