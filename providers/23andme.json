{
	"name": "23AndMe",
	"url": "https://api.23andme.com",
	"oauth2": {
		"authorize": "/authorize/",
		"access_token": {
			"url": "/token/",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string",
			"scope": "string"
		}
	}
}