{
	"name": "Soundcloud",
	"api_url": "https://api.soundcloud.com/",
	"oauth2": {
		"authorize": "https://soundcloud.com/connect",
		"access_token": {
			"url": "https://api.soundcloud.com/oauth2/token",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string",
			"scope": "string"
		}
	}
}