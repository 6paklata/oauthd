{
	"name": "DeviantArt",
	"url": "https://www.deviantart.com/oauth2/draft15",
	"oauth2": {
		"authorize": {
			"url": "/authorize",
			"query": {
				"client_id": "{client_id}",
				"response_type": "code",
				"redirect_uri": "{{callback}}"
			}
		},
		"access_token": {
			"url": "/token",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string"
		}
	},
	"href": {
		"provider": "http://deviantart.com/",
		"docs": "http://www.deviantart.com/developers/",
		"keys": "http://www.deviantart.com/submit/app/"
	}
}