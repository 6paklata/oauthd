{
	"name": "MailChimp",
	"url": "https://login.mailchimp.com/oauth2",
	"oauth2": {
		"authorize": {
		"url":"/authorize",
			"query": {
				"client_id": "{client_id}",
				"response_type": "code",
				"redirect_uri": "{{callback}}",
				"state": "{{state}}",
				"scope": ""
			}

		},
		"access_token": {
			"url": "/token",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string"
		}
	},
	"href": {
		"provider": "http://mailchimp.com/",
		"docs": "http://apidocs.mailchimp.com/",
		"keys": "https://us6.admin.mailchimp.com/account/api/"
	}
}