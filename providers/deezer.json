{
	"name": "Deezer",
	"url": "https://connect.deezer.com/oauth",
	"oauth2": {
		"authorize": {
			"url": "/auth.php",
			"format": "json",
			"query": {
				"app_id": "{app_id}",
				"response_type": "code",
				"redirect_uri": "{{callback}}",
				"perms": "{perms}"
			}
		},
		"access_token": {
			"url": "/access_token.php",
			"query": {
				"app_id": "{app_id}",
				"secret": "{secret}",
				"grant_type": "authorization_code",
				"code": "{{code}}"
			}
		},
		"request": {
			"query": {
				"access_token": "{{token}}"
			}
		},
		"parameters": {
			"app_id": "string",
			"secret": "string",
			"perms": "string"
		}
	}
}