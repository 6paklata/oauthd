{
	"name": "DailyMile",
	"url": "https://api.dailymile.com/oauth",
	"oauth2": {
		"authorize": "/authorize",
		"access_token": {
			"url": "/token",
			"method": "post"
		},
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"parameters": {
			"client_id": "string",
			"client_secret": "string"
		}
	},
	"href": {
		"provider": "http://dailymile.com",
		"docs": "http://www.dailymile.com/api/documentation",
		"keys": "http://www.dailymile.com/api/consumers/new"
	}
}