{
	"name": "Mail.ru",
	"url": "https://connect.mail.ru/oauth",
	"oauth2": {
		"authorize": {
			"url": "/authorize",
			"query": {
				"client_id": "{client_id}",
				"response_type": "code",
				"redirect_uri": "{{callback}}"
			}
		},
		"access_token": "/token",
		"request": {
			"query": {
				"oauth_token": "{{token}}"
			}
		},
		"refresh": "/token"
	},
	"href": {
		"keys": "http://api.mail.ru/sites/my/add/",
		"docs": "http://api.mail.ru/docs/",
		"apps": "http://api.mail.ru/sites/my/",
		"provider": "http://api.mail.ru/"
	}
}